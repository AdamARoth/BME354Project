void exportData()
{
  boolean continueExport = true;
  byte memAccessIndex = 7;
  while(continueExport)
  { 
    elapsedTime = (int) (((memAccessIndex-1)/2)-3)*5; //Produces elapsed time in seconds
    setTemp = (int) EEPROM.read(memAccessIndex);
    currTemp = (int) EEPROM.read(memAccessIndex+1);
    if(currTemp == 0 || setTemp == 0 || memAccessIndex+3 > 255)
    {
      continueExport = false;
    }
    else
    {  
      exportMATLAB();
    }
    memAccessIndex = memAccessIndex + 2;
  }
  elapsedTime = 0;
}
